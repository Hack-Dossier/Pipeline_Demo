Jenkinsfile (Declaritive Pipeline)
/*Require docker pipeline 
pipeline
{
  agents
  {
    stages{
      stage(builds)
      {
        steps
        {
        sh "mvn --version"
        }
      }
      stage(Testing)
      {
        steps
        {
        echo  "testing..."
        }
      }
      stage(Deploy)
      {
        steps
        {
        echo "Deploying"
        }
      }
    }
  }
}
